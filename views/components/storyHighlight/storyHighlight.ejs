
<div class="story--container">
	<div class="story--container-text">
		<h1><%= story.title %></h1>
		<p><%= story.text %></p>
	</div>
	<div class="story--container-image">
		<% if ( story.image ) { %>
			<img src="<%= story.image %>" alt="">
		<% } else { %>
			<img src="/images/example-detail.jpg" alt="">
		<% } %>
	</div>
</div>