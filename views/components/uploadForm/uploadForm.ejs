<form method="post" enctype="multipart/form-data" action="/story-upload/upload">
	<label for="title">Titel</label>
	<input type="text" name="title" required>

	<label for="storyText">Story text</label>
	<textarea name="storyText"></textarea>

	<label for="upload">Upload a file</label>
	<input type="file" name="upload" multiple>

	<label for="email">E-mail</label>
	<input type="email" name="email">

	<label for="phone">Telefoon nummer</label>
	<input type="text" name="phone">

	<label for="tags">Tags</label>
	<input type="text" name="tags">

	<label for="location">Locatie</label>
	<input type="text" name="location">
	
	<label for="time">Tijd van verhaal</label>
	<input type="text" name="time">

	<input type="submit" class="button">
</form>
