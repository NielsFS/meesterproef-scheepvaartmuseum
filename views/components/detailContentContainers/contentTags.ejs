<div class="tag--container">
	<% if ( story.allTags.length ) { %>
	<p>Tags: </p>
	<ul class="content--container-tags">
		<% story.allTags.forEach( function( el ) { %>
			<li><a href="/story-overview?tag=<%= el.name %>"><%= el.name %> (<%= el.count %>)</a></li>
		<% } ) %>
	</ul>
	<% } else { %>
		<p>Tags: There are no tags</p>
	<% } %>
</div>
